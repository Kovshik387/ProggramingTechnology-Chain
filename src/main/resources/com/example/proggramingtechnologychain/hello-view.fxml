<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vBoxField" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proggramingtechnologychain.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label prefHeight="18.0" prefWidth="197.0" text="Введите пароль для проверки" />
      <TextField fx:id="fieldText" alignment="TOP_CENTER" />
      <Button fx:id="checkButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="110.0" text="Проверить" />
   </children>
</VBox>
